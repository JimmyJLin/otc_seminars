<table class="highlight">
  <thead>
    <tr>
        <th>Type</th>
        <th>Date</th>
        <th>Details</th>
    </tr>
  </thead>

  <% if(classData === undefined) { %>
    <tbody>
      <td></td>
      <td class="center">Please add addditional seminars</td>
      <td></td>
    </tbody>
  <% } else { %>
    <% classData.forEach((el) => { %>
      <tbody>
        <td>
          <% switch ( el.class_type ) {
            case "handcraft_3days": %>
              Handcraft 3 Days Seminar
              <% break;
            case "handcraft_5days": %>
              Handcraft 5 Days Seminar
              <% break;
            case "machine_3days": %>
              Machine 3 Days Seminar
              <% break;
            case "machine_5days": %>
              Machine 5 Days Seminar
              <% break;
            case "6D_Hybrid": %>
              6D Hybrid 5 Days Seminar
              <% break;
            case "powdered": %>
              Advanced Powdered
              <% break;
            case "lips": %>
              Advanced Lips
              <% break;
            case "eyeliners": %>
              Advanced Eyeliners
              <% break;
            case "areola": %>
              Advanced Areola
              <% break;
            case "hairline": %>
              Advanced Hairline
              <% break;
            case "others": %>
              Others
              <% break;
            default: %>
              No Seminar
              <% break;
           } %>

          <!-- <%= el.class_type %> -->
        </td>
        <td><%= el.class_date %></td>
        <td>
          <a class="waves-effect btn waves-light" href='/seminars/class/<%= el.id %>'><i class="material-icons center">dashboard</i></a>
        </td>
      </tbody>
      <% }) %>
  <% } %>

</table>
